{
  "name": "movie-stats-web",
  "version": "0.1.0",
  "scripts": {
    "build": "webpack --config ./config/webpack.prod.js",
    "deploy": "npm run build && now --name imdb-movie-stats --static --public dist",
    "format": "prettier --jsx-bracket-same-line true --print-width 100 --single-quote --tab-width 4 --write '{config,src,test}/**/*.{js,json}'",
    "start": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "test": "jest test"
  },
  "description": "IMDb movie stats",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugo-cardenas/movie-stats-web.git"
  },
  "author": "Hugo CÃ¡rdenas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hugo-cardenas/movie-stats-web/issues"
  },
  "homepage": "https://github.com/hugo-cardenas/movie-stats-web#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|less|styl)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "prettier": "^1.9.2",
    "raf": "^3.4.0",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bulma": "^0.6.0",
    "chart.js": "^2.7.1",
    "chroma-js": "^1.3.4",
    "chrono-node": "^1.3.5",
    "lodash": "^4.17.4",
    "papaparse": "^4.3.6",
    "randomcolor": "^0.5.3",
    "react": "^16.2.0",
    "react-chartjs": "^0.8.0",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.14",
    "tippy.js": "^1.3.0"
  }
}
